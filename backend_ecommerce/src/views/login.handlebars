<div class="firstContainer">
  <h1 class="text-center titleCustomizeDos">Inicio de Sesión</h1>

  <div class="formContainer shadow p-3 mb-5 rounded">

    <form id="form" action="/static/login" method="POST">

      <input type="email" required name="email" placeholder="Email" />

      <input
        type="password"
        required
        name="password"
        placeholder="Contraseña"
      />

      <button class="btn btn-outline-warning" type="submit">Iniciar Sesión</button>
    </form>
  </div>

</div>